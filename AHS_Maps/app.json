{
  "expo": {
    "name": "AHS Maps",
    "slug": "map-availability",
    "version": "1.2",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "plugins": [
        [
          "expo-location",
          {
            "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
          }
        ]
      ]
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "$(PRODUCT_NAME) needs your location to enable safety features and restrict map access to Allen High School users only. Also, it is used for directional guidance.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "$(PRODUCT_NAME) needs your location to enable safety features and restrict map access to Allen High School users only. Also, it is used for directional guidance.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "newArchEnabled": true,
      "bundleIdentifier": "com.rishinigam.mapavailability"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.rishinigam.mapavailability"
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "1a25fcd1-37fe-455c-ac97-71b24627c4e6"
      }
    },
    "owner": "rishinigam"
  }
}
